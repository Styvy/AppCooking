BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS `recette` (
	`id`	INTEGER PRIMARY KEY AUTOINCREMENT,
	`title`	TEXT,
	`ingredient`	TEXT,
	`recipe`	TEXT
);
CREATE TABLE IF NOT EXISTS `produit` (
	`id`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	`qty`	INTEGER,
	`nom`	TEXT,
	`code_ban`	INTEGER,
	`img`	INTEGER,
	`categorie`	INTEGER,
	`disponible`	INTEGER NOT NULL
);
COMMIT;
